    <body class="container">
        <header>
            <%- include('../partials/navbar'); %>
        </header>
        <main>
            <br />
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Contact Information</h5>
                    <br />
                    <h6 class="card-subtitle mb-2">Office Hours</h6>
                    <p class="card-text">Monday-Friday, 9am to 6pm Local Time</p>
                    <h6 class="card-subtitle mb-2">Phone Number</h6>
                    <p class="card-text">+00 (123) 456-7890</p>
                    <br />
                    <h6 class="card-subtitle mb-2">Email</h6>
                    <p class="card-text">contact@example.com</p>
                    <h6 class="card-subtitle mb-2">Mailing Address</h6>
                    <p class="card-text">123 Example Road, Town State, Zip, Country</p>
                </div>
            </div>
        </main>
        <footer>
            <%- include('../partials/footer'); %>
        </footer>
